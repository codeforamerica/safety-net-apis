{
  "name": "@safety-net/harness-app",
  "version": "1.0.0",
  "description": "Running application that wires form contracts to the mock API server via generated TypeScript clients",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "generate:api": "node ../clients/scripts/generate-clients-typescript.js --specs=../contracts --out=./src/generated/api"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@trussworks/react-uswds": "^10.0.0",
    "axios": "^1.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.0",
    "react-router-dom": "^7.1.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.1",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.3.3",
    "vite": "^6.0.0"
  }
}
